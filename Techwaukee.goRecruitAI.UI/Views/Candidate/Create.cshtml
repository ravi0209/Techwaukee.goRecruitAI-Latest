<script>
    var currentStep = 1;
    $(document).ready(function () {

        //var prevStep = 0;
        $("#div_Loader").hide();
        $('#lblPagetitle').text("Add Candidate");
        $('#lblPagetitle1').text("Add Candidate");

    })

    function ChangeNextStep() {
        
        currentStep = currentStep + 1;
        alert(currentStep);
        if (currentStep > 1) {
            
            $("#divSection" + currentStep).show();
            $("#divSection" + (currentStep - 1)).hide();
            $("#btnStep" + currentStep).addClass("js-active");
            $("#btnPrev").show();
            if (currentStep >= 5) {
                $("#btnNext").hide();
            }
        }
    }
    function ChangePrevStep() {
        
        if (currentStep > 1) {
            $("#divSection" + (currentStep - 1)).show();
            $("#divSection" + currentStep).hide();
            $("#btnStep" + currentStep).removeClass("js-active");
            currentStep = currentStep - 1;
            $("#btnNext").show();
            if (currentStep == 1) {
                $("#btnPrev").hide();
            }
        }
    }
</script>
